@model POS.ViewModel.SupplierModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}
<div>
    <h2>Detail Supplier</h2>
    <table class="table table-bordered">
        <tr>
            <th>Id</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Company Name</th>
            <td>@Model.CompanyName</td>
        </tr>
        <tr>
            <th>ContactName</th>
            <td>@Model.ContactName</td>
        </tr>
        <tr>
            <th>Contact Title</th>
            <td>@Model.ContactTitle</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>@Model.Address</td>
        </tr>
        <tr>
            <th>City</th>
            <td>@Model.City</td>
        </tr>
        <tr>
            <th>Region</th>
            <td>@Model.Region</td>
        </tr>
        <tr>
            <th>PostalCode</th>
            <td>@Model.PostalCode</td>
        </tr>
        <tr>
            <th>Country</th>
            <td>@Model.Country</td>
        </tr>
        <tr>
            <th>Phone</th>
            <td>@Model.Phone</td>
        </tr>
        <tr>
            <th>Fax</th>
            <td>@Model.Fax</td>
        </tr>
        <tr>
            <th>Home Page</th>
            <td>@Model.HomePage</td>
        </tr>
    </table>

</div>
